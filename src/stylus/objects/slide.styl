.slide
  position relative
  min-height 100%
  overflow hidden

.slide__bg
  position absolute
  left 0
  top 0
  size 100% 100%  
  background-position center
  background-size cover

.slide--1905
  background black


.slide__bg--1848
  background-image url('../images/1848.jpg')

.slide__bg--1905
  background-image url('../images/1905-1.jpg')
